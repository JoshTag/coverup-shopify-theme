<div class="product-block product-icons-ctn">
  {% for icon in product.metafields.tab_icon.icon.value %}
    {% assign icon_url = icon | replace: " ", "_" | append: '_notext.png' %}
    <div class="product-icons__icon-tabs">
      <div class="product-icons__icon-img" style="background-image: url({{ icon_url | file_img_url: '300x300' }}"></div>
      <span class="product-icons__icon-text">{{ icon }}</span>
    </div>
  {% endfor %}
</div>